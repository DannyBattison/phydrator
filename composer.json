{
    "name": "dannyb/phydrator",
    "description": "PHP entity hydration",
    "type": "library",
    "license": "MIT",
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "authors": [
        {
            "name": "Danny Battison",
            "email": "captain@danny.gg"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/annotations": "^1.11",
        "haydenpierce/class-finder": "^0.4.3"
    },
    "autoload": {
        "psr-4": {
            "PHydrator\\": "src",
            "PHydrator\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests --colors=always"
    }
}
